
	<%= link_to 'New Book', new_book_path %>
	<div>
  <h2> Book Index </h2>
  <% @books.each do |book| %>
  Title: <%= book.title %> <br>
	Author: <%= book.author %> <br>
	<%= image_tag "#{book.image_url}" %> <br>
	<%= link_to '🔎', book_path(book) %>
	<%= link_to '❌',book_path(book), method: :delete, data: { confirm: 'Do you really want to delete this?'} %>
	</div><br>
	<% end %>
	<%= will_paginate @books %>