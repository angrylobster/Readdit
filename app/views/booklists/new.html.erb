<%= @users[0].id %>


<%= form_with scope: :booklist, url: booklists_path, local: true do |form| %>


	<%= form.hidden_field :user_id, value: current_user.id %>
  <%= form.collection_check_boxes :book_ids, @books, :id, :title %>

  <p>
	<%= form.label :booklist_type %><br>
	<%= form.text_area :booklist_type %>
  </p>


  <p>
	<%= form.submit %>
  </p>
<% end %>

<%= link_to 'Go back to booklist index', booklists_path %>